{
  "name": "templateforv2-static",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"yarn watch:tsc\" \"yarn watch:server\" \"yarn watch:client\"",
    "watch:tsc": "tsc --watch",
    "watch:server": "nodemon --watch dist ./dist/index.js",
    "watch:client": "yarn watchify ./dist/client/index.js  --standalone Bundle -o ./wwwroot/dist/bundle.js -v",
    "build": "echo This is a static template, there is no bundler or bundling involved!"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesandbox-app/static-template.git"
  },
  "keywords": [],
  "author": "Ives van Hoorne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codesandbox-app/static-template/issues"
  },
  "homepage": "https://github.com/codesandbox-app/static-template#readme",
  "dependencies": {
    "@kitajs/html": "^3.0.4",
    "@kitajs/ts-html-plugin": "^1.3.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "browserify": "^17.0.0",
    "concurrently": "^8.2.1",
    "watchify": "^4.0.0"
  }
}
